outfile=$(basename $0)
dirfile=$(dirname $0)
cd $dirfile

../../../vdptest -v -v -v -F vm.cfg -i veth0 -Cnew=vmxxx,name=vm201,typeid=203 -p vmxxx,e=18 -s,w=5
rc=$?
exit $rc
